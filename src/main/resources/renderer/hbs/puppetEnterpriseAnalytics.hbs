<div class="analytics-data-view">
	<div class="report-header">
		<h2>Bolt Automation Analytics</h2>
	</div>
	<div class="data-view">
	</div>
	<div class="break-container">
			<h4>Job Analytics</h4>
			<div class="instance-type-list analytics-instance-types" data-status="" data-server-type="" data-offset="" data-max="" data-phrase="" data-sort="" data-direction="">
				<table class="table server-list-table hidden-xs">
					<thead>
						<tr>
							<th style="width:200px;">ID</th>
							<th>Status</th>
							<th>User</th>
							<th>Nodes</th>
						</tr>
					</thead>
					<tbody>
						{{#each data.jobs}}
							<tr>
							<td>{{id}}</td>
							<td>{{status}}</td>
							<td>{{user}}</td>
							<td>{{nodes}}</td>
							</tr>
						{{/each}}
					</tbody>
				</table>
			</div>
	</div>
</div>

<script nonce="{{webnonce}}">
   var csrfValue = document.querySelector("meta[name='_csrf']").attributes['content'].value;
   console.log(csrfValue)
    var csrfEl = document.getElementById("pebolt");
    csrfEl.value = csrfValue;
</script>
